<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="semana7.Login">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="BIENVENIDO AL SISTEMA" />
            <Entry x:Name="txtUsuario" Placeholder="Ingrese el Usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese la contrasena"/>
            <Button x:Name="btnIniciar" Text="Iniciar seccion" Clicked="btnIniciar_Clicked"/>
            <Button x:Name="btnRegistrar" Text="Registrar" Clicked="btnRegistrar_Clicked"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>