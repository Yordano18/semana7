<?xml version="1.0" encoding="utf-8" ?>
<ContentPage xmlns="http://xamarin.com/schemas/2014/forms"
             xmlns:x="http://schemas.microsoft.com/winfx/2009/xaml"
             x:Class="semana7.Registro">
    <ContentPage.Content>
        <StackLayout>
            <Label Text="Ventana ed Registro" />
            <Entry x:Name="txtNombre" Placeholder="Ingrese Nombre"/>
            <Entry x:Name="txtUsuario" Placeholder="Ingrese Usuario"/>
            <Entry x:Name="txtContrasena" Placeholder="Ingrese la Contrasena"/>
            <Button x:Name="btnAgregar" Text="Agregar" Clicked="btnAgregar_Clicked"/>
            
        </StackLayout>
    </ContentPage.Content>
</ContentPage>